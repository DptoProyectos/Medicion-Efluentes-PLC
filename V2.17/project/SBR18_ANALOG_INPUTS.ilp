(*2*)
(* Network 0 *)
(*YCHREAD ANALOG INPUTSLeo las entradas analogicas mediante un proceso de multiplexadoTEMP. PH, ORP,  [ CAUDAL | DISTANCIA DEL SENOR AL AGUA ]State  = 2Ejecuto la lectura solo si está habilitado el RunSgn*)
LD    %M103.1
CAL    SBR09    ReadAin, VirtualAIn0, VirtualAIn1, VirtualAIn2, VirtualAIn3, AInBusy
CAL    SBR27    AI0_Offset, AI0_Mmax, AI0_Mmin, VirtualAIn0, AI0_Imax, AI0_Imin, AI0_Alarm, AI0_AlrLow, AI0_mgn, AI0_AlrHigh
CAL    SBR10    AI1_Offset, AI1_Mmax, AI1_Mmin, VirtualAIn1, AI1_Imax, AI1_Imin, AI1_Alarm, AI1_AlrLow, AI1_mgn, AI1_AlrHigh
CAL    SBR19    AI2_Offset, AI2_Mmax, AI2_Mmin, VirtualAIn2, AI2_Imax, AI2_Imin, AI2_Alarm, AI2_AlrLow, AI2_mgn, AI2_AlrHigh
CAL    SBR20    AI3_Offset, AI3_Mmax, AI3_Mmin, VirtualAIn3, AI3_Imax, AI3_Imin, AI3_Alarm, AI3_AlrLow, AI3_mgn, AI3_AlrHigh
(* Network 1 *)
(*YCHCuando se esta en mantenimiento y el metodo de medicion de caudal es indirecto se calcula la altura del agua para que se refresque el dato en el HMI*)
LD    %V3648.0
NE    CaudMethod, 0
MOVE    AI3_mgn, CauRaw
(* Network 2 *)
(*YCHCalculo la altura del agua en el canal por si se fuese a necesitar*)
LD    TRUE
MOVE    CauRaw, DCurrent
MOVE    DCurrent, DCurrentx1000
MUL    1000.0, DCurrentx1000
R_TO_DI    DCurrentx1000, DCurrentDint
DI_TO_I    DCurrentDint, IntDist
LINCO    DMin, DMax, HMax, HMin, 1.0, IntDist, dint5, HOut
(* Network 3 *)
(*YCHACTIVACIÓN DE ALARMASCondiciones para desactivar las alarmas en el HMI*)
LDN    %M100.7
ANDN    %M101.0
ANDN    %M101.1
AND(
LDN    %M101.2
OR(
LD    TRUE
NE    CaudMethod, 0
)
OR(
LD    TRUE
NE    TypeFlow, 0
)
)
R    %M103.0
(* Network 4 *)
(*YCHACTIVACIÓN DE ALARMASCondiciones para activar las alarmas en el HMI*)
LD    %V3648.3
AND    %M100.7
OR(
LD    %V3648.4
AND    %M101.0
)
OR(
LD    %V3648.5
AND    %M101.1
)
OR(
LD    %V3648.2
EQ    CaudMethod, 0
EQ    TypeFlow, 0
AND    %M101.2
)
S    %M103.0
