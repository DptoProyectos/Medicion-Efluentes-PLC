(*2*)
(* Network 0 *)
(*DRIVERSMANEJO DE LA BOMBA DE MUESTREO*)
LD    %SM0.0
EQ    SAMPLE_PUMP, 1
ST   OUT1
(* Network 1 *)
(*DRIVERSMANEJO DE LA BOMBA DE LIMPIEZA*)
LD    %SM0.0
EQ    CLEAN_PUMP, 1
ST   OUT2
(* Network 2 *)
(*DRIVERSMANEJO DE LA SEÑAL PARA INVERTIR LA ALIMENTACIÓN DE LA BOMBA*)
LD    %SM0.0
EQ    SWITCH_PUMP, 1
ST   OUT4
(* Network 3 *)
(*DRIVERSMANEJO DE LA SEÑAL DE POLLEO*)
LD    %SM0.0
EQ    SIGNAL_PULL, 1
ST   OUT3
