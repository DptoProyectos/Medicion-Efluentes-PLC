VAR_INPUT
    OMODE  AT  %L0.6 : BOOL ; (* MODO EN QUE VA A TRABAJAR EL CANAL ANALOGICO DE SALIDA {0=>MODO NORMAL | 1=MODO CALIBRACION} *) 
    OBTINC  AT  %L0.7 : BOOL ; (* BOTON QUE EN CADA TRANSICION (0->1) INCREMENTA EL VALOR DE AJUSTE DE CORRIENTE DEL CANAL AO_0 *) 
    OBTDEC  AT  %L1.0 : BOOL ; (* BOTON QUE EN CADA TRANSICION (0->1) DECREMENTA EL VALOR DE AJUSTE DE CORRIENTE DEL CANAL AO_0 *) 
    OCALIB  AT  %L2.4 : BOOL ; (* BIT QUE IDICA EL MODO DE CALIBRACION {0->CERO | 1->SPAN} *) 
END_VAR
VAR_OUTPUT
    OADJ  AT  %LD80 : REAL ; (* VALOR QUE MUESTRA EL AJUSTE QUE SE ESTA REALIZANDO MIENTRAS SE CALIBRA *) 
END_VAR
